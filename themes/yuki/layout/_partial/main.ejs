<div class="container">
  <div class="main">
    <% page.posts.sort("date", "desc").each(function(item){ %>
    <%- partial('_partial/section', {item: item}) %>
    <% }) %>
    <% if (page.total > 1){ %>
    <nav id="page-nav">
        <%- paginator({
            prev_text: '&laquo; Prev',
            next_text: 'Next &raquo;'
        }) %>
    </nav>
    <% } %>    
  </div>
</div>